<userDefinedSQL>WITH regionais AS&lt;br/&gt;  (SELECT REGIONAL.NOME AS regional FROM REGIONAL WHERE indicador_smp = &apos;T&apos;&lt;br/&gt;  ),&lt;br/&gt;  dias AS&lt;br/&gt;  (SELECT data&lt;br/&gt;  FROM ddx&lt;br/&gt;  WHERE TO_CHAR(ddx.data, &apos;MM/yyyy&apos;) = TO_CHAR(SYSDATE, &apos;MM/yyyy&apos;)&lt;br/&gt;  ),&lt;br/&gt;  regionais_x_dias_x_pmms AS&lt;br/&gt;  (SELECT * FROM regionais r, dias d&lt;br/&gt;  ),&lt;br/&gt;  dados_traducao_sem_vinculo AS&lt;br/&gt;  (SELECT data, uf, campo_39, campo_40 FROM view_smp10_smp11_dados&lt;br/&gt;  ),&lt;br/&gt;  dados_x_pmm AS&lt;br/&gt;  (SELECT tab.data,&lt;br/&gt;    regionais.Regional,&lt;br/&gt;    NVL(TO_CHAR(SUM(NVL(tab.campo_39, 0))), &apos;-&apos;) AS campo_39,&lt;br/&gt;    NVL(TO_CHAR(SUM(NVL(tab.campo_40, 0))), &apos;-&apos;) AS campo_40&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT DISTINCT Tab.data AS Data,&lt;br/&gt;      Reg.NOME                AS Regional,&lt;br/&gt;      uf.nome                 AS uf&lt;br/&gt;    FROM dados_traducao_sem_vinculo Tab&lt;br/&gt;    INNER JOIN uf uf&lt;br/&gt;    ON uf.nome = Tab.uf&lt;br/&gt;    INNER JOIN Cn Cn&lt;br/&gt;    ON Cn.id_uf = uf.id_uf&lt;br/&gt;    INNER JOIN Metricview.Rel_Regional_Cod_Nacional Reg_Cod&lt;br/&gt;    ON Reg_Cod.Id_Cn = Cn.Id_Cn&lt;br/&gt;    INNER JOIN REGIONAL Reg&lt;br/&gt;    ON Reg_Cod.Id_Regional = Reg.ID_REGIONAL&lt;br/&gt;    AND Reg.indicador_smp  = &apos;T&apos;&lt;br/&gt;    ) regionais,&lt;br/&gt;    dados_traducao_sem_vinculo tab&lt;br/&gt;  WHERE tab.uf = regionais.uf&lt;br/&gt;  AND tab.data = regionais.Data&lt;br/&gt;  GROUP BY tab.data,&lt;br/&gt;    regionais.Regional&lt;br/&gt;  ),&lt;br/&gt;  dados_finais AS&lt;br/&gt;  (SELECT reg.data,&lt;br/&gt;    reg.regional,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN dados.campo_40 = &apos;0&apos;&lt;br/&gt;      AND dados.campo_39 != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN dados.campo_40 = &apos;-&apos;&lt;br/&gt;      AND dados.campo_39  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN dados.campo_40 != &apos;-&apos;&lt;br/&gt;      AND dados.campo_39   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND((dados.campo_39 * 100 / dados.campo_40), 2), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END            AS smp,&lt;br/&gt;    dados.campo_39 AS smp_a,&lt;br/&gt;    dados.campo_40 AS smp_b&lt;br/&gt;  FROM dados_x_pmm dados,&lt;br/&gt;    regionais_x_dias_x_pmms reg&lt;br/&gt;  WHERE reg.regional               = dados.Regional(+)&lt;br/&gt;  AND TO_CHAR(reg.data, &apos;MM/yyyy&apos;) = dados.data(+)&lt;br/&gt;  )&lt;br/&gt;SELECT dados_finais.data,&lt;br/&gt;  dados_finais.regional,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN dados_finais.smp          != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(dados_finais.smp) &gt; 100&lt;br/&gt;    THEN &apos;100,00&apos;&lt;br/&gt;    ELSE REPLACE(dados_finais.smp, &apos;.&apos;, &apos;,&apos;)&lt;br/&gt;  END AS smp,&lt;br/&gt;  dados_finais.smp_a,&lt;br/&gt;  dados_finais.smp_b&lt;br/&gt;FROM dados_finais&lt;br/&gt;ORDER BY 1,&lt;br/&gt;  2,&lt;br/&gt;  3</userDefinedSQL>
