<userDefinedSQL>WITH dados_traducao_sem_vinculo AS&lt;br/&gt;  (SELECT VIEW_MSC_VINCULO_SMP7.DATA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.HORA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.PMM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.CN,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.MSC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.OK1_DRP,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.OK1_DRP_TCT,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.OKT,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP7.OK_ITCP&lt;br/&gt;  FROM VIEW_MSC_VINCULO_SMP7&lt;br/&gt;  WHERE VIEW_MSC_VINCULO_SMP7.CODIGO != &apos;NID&apos;&lt;br/&gt;  ),&lt;br/&gt;  dados_smp_UF AS&lt;br/&gt;  (SELECT Data,&lt;br/&gt;    Pmm,&lt;br/&gt;    UF,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN SMP7_B = &apos;-&apos;&lt;br/&gt;      AND Smp7_A  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN (SMP7_B = &apos;-&apos;&lt;br/&gt;      OR SMP7_B    = &apos;0&apos;)&lt;br/&gt;      AND Smp7_A  != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP7_B           != &apos;-&apos;&lt;br/&gt;      AND TO_NUMBER(SMP7_B) &gt;= 0&lt;br/&gt;      AND Smp7_A             = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND(((Smp7_A / SMP7_B) * 100), 2), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS smp7,&lt;br/&gt;    Smp7_A,&lt;br/&gt;    SMP7_B,&lt;br/&gt;    nome_extenso&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT TRUNC(Tab.DATA) AS Data,&lt;br/&gt;      pmm.NOME              AS Pmm,&lt;br/&gt;      uf.nome               AS UF,&lt;br/&gt;      uf.nome_extenso,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.OK1_DRP, 0) + NVL(Tab.OK1_DRP_TCT, 0))), &apos;-&apos;) AS Smp7_A,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.OKT, 0)     - NVL(Tab.OK_ITCP, 0))), &apos;-&apos;)     AS SMP7_B&lt;br/&gt;    FROM VIEW_MSC_CN_ATIVO_SMP7 b,&lt;br/&gt;      dados_traducao_sem_vinculo Tab&lt;br/&gt;    INNER JOIN PMM pmm&lt;br/&gt;    ON Tab.HORA BETWEEN pmm.INICIO AND pmm.FIM&lt;br/&gt;    INNER JOIN CN cn&lt;br/&gt;    ON cn.nome = Tab.CN&lt;br/&gt;    INNER JOIN UF uf&lt;br/&gt;    ON uf.id_uf                                          = cn.id_uf&lt;br/&gt;    WHERE Tab.MSC                                        = b.MSC&lt;br/&gt;    AND Tab.CN                                           = b.CN&lt;br/&gt;    AND to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO&lt;br/&gt;    AND b.STATUS                                         = &apos;T&apos;&lt;br/&gt;    GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;      pmm.NOME,&lt;br/&gt;      uf.nome,&lt;br/&gt;      uf.nome_extenso&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_smp_uf AS&lt;br/&gt;  (SELECT *&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.Data FROM dados_smp_uf&lt;br/&gt;    ) datas,&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.Pmm FROM dados_smp_uf&lt;br/&gt;    ) pmms,&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.UF, dados_smp_uf.nome_extenso FROM dados_smp_uf&lt;br/&gt;    ) ufs&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_por_dados AS&lt;br/&gt;  (SELECT agrupadores.Data,&lt;br/&gt;    agrupadores.UF,&lt;br/&gt;    agrupadores.Pmm,&lt;br/&gt;    agrupadores.nome_extenso,&lt;br/&gt;    TO_CHAR(DECODE(dados.Smp7_A, NULL, &apos;-&apos;, dados.Smp7_A)) smp7_a,&lt;br/&gt;    TO_CHAR(DECODE(dados.SMP7_B, NULL, &apos;-&apos;, dados.SMP7_B)) smp7_b,&lt;br/&gt;    TO_CHAR(DECODE(dados.smp7, NULL, &apos;-&apos;, dados.smp7)) smp7&lt;br/&gt;  FROM dados_smp_uf dados,&lt;br/&gt;    agrupadores_smp_uf agrupadores&lt;br/&gt;  WHERE agrupadores.Data       = dados.Data(+)&lt;br/&gt;  AND agrupadores.Pmm          = dados.Pmm(+)&lt;br/&gt;  AND agrupadores.UF           = dados.UF(+)&lt;br/&gt;  AND agrupadores.nome_extenso = dados.nome_extenso(+)&lt;br/&gt;  UNION ALL&lt;br/&gt;  SELECT a.*,&lt;br/&gt;    TO_CHAR(DECODE(a.smp7_b, NULL, 0, DECODE(a.smp7_b, 0, 0, (DECODE(a.smp7_a, NULL, 0, a.smp7_a) * 100 / a.smp7_b))))&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT agrupadores.Data,&lt;br/&gt;      agrupadores.UF,&lt;br/&gt;      &apos;PMM_TOTAL&apos; PMM,&lt;br/&gt;      agrupadores.nome_extenso,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.Smp7_A, NULL, 0, dados.Smp7_A))) smp7_a,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP7_B, NULL, 0, dados.SMP7_B))) smp7_b&lt;br/&gt;    FROM dados_smp_uf dados,&lt;br/&gt;      agrupadores_smp_uf agrupadores&lt;br/&gt;    WHERE agrupadores.Data       = dados.Data(+)&lt;br/&gt;    AND agrupadores.UF           = dados.UF(+)&lt;br/&gt;    AND agrupadores.Pmm          = dados.Pmm(+)&lt;br/&gt;    AND agrupadores.nome_extenso = dados.nome_extenso(+)&lt;br/&gt;    GROUP BY agrupadores.Data,&lt;br/&gt;      agrupadores.UF,&lt;br/&gt;      agrupadores.nome_extenso&lt;br/&gt;    ) a&lt;br/&gt;  )&lt;br/&gt;SELECT TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA,&lt;br/&gt;  agrupadores_por_dados.UF,&lt;br/&gt;  agrupadores_por_dados.Pmm,&lt;br/&gt;  NVL(agrupadores_por_dados.nome_extenso, &apos; &apos;)  AS nome,&lt;br/&gt;  REPLACE(agrupadores_por_dados.smp7, &apos;.&apos;, &apos;,&apos;) AS SMP,&lt;br/&gt;  agrupadores_por_dados.smp7_a                  AS SMP_A,&lt;br/&gt;  agrupadores_por_dados.smp7_b                  AS SMP_B&lt;br/&gt;FROM agrupadores_por_dados&lt;br/&gt;ORDER BY 2,&lt;br/&gt;  1,&lt;br/&gt;  7</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;With dados_traducao_sem_vinculo As (Select VIEW_MSC_VINCULO_SMP7.DATA, VIEW_MSC_VINCULO_SMP7.HORA, VIEW_MSC_VINCULO_SMP7.PMM, VIEW_MSC_VINCULO_SMP7.CN, VIEW_MSC_VINCULO_SMP7.MSC, VIEW_MSC_VINCULO_SMP7.OK1_DRP, VIEW_MSC_VINCULO_SMP7.OK1_DRP_TCT, VIEW_MSC_VINCULO_SMP7.OKT, VIEW_MSC_VINCULO_SMP7.OK_ITCP From VIEW_MSC_VINCULO_SMP7 Where VIEW_MSC_VINCULO_SMP7.CODIGO != &apos;NID&apos;), dados_smp_UF As (Select Data, Pmm, UF, Case When SMP7_B = &apos;-&apos; And Smp7_A = &apos;-&apos; Then &apos;-&apos; When (SMP7_B = &apos;-&apos; Or SMP7_B = &apos;0&apos;) And Smp7_A != &apos;-&apos; Then &apos;0,00&apos; When SMP7_B != &apos;-&apos; And TO_NUMBER(SMP7_B) &gt;= 0 And Smp7_A = &apos;-&apos; Then &apos;0,00&apos; Else NVL(trim(TO_CHAR(ROUND(((Smp7_A / SMP7_B) * 100), 2), &apos;999990D99&apos;)), &apos;-&apos;) End As smp7, Smp7_A, SMP7_B, nome_extenso From (Select TRUNC(Tab.DATA) As Data, pmm.NOME As Pmm, uf.nome As UF, uf.nome_extenso, NVL(TO_CHAR(Sum(NVL(Tab.OK1_DRP, 0) + NVL(Tab.OK1_DRP_TCT, 0))), &apos;-&apos;) As Smp7_A, NVL(TO_CHAR(Sum(NVL(Tab.OKT, 0) - NVL(Tab.OK_ITCP, 0))), &apos;-&apos;) As SMP7_B From VIEW_MSC_CN_ATIVO_SMP7 b, dados_traducao_sem_vinculo Tab Inner Join PMM pmm On Tab.HORA Between pmm.INICIO And pmm.FIM Inner Join CN cn On cn.nome = Tab.CN Inner Join UF uf On uf.id_uf = cn.id_uf Where Tab.MSC = b.MSC And Tab.CN = b.CN And to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO And b.STATUS = &apos;T&apos; Group By TRUNC(Tab.DATA), pmm.NOME, uf.nome, uf.nome_extenso)), agrupadores_smp_uf As (Select * From (Select Distinct dados_smp_uf.Data From dados_smp_uf) datas, (Select Distinct dados_smp_uf.Pmm From dados_smp_uf) pmms, (Select Distinct dados_smp_uf.UF, dados_smp_uf.nome_extenso From dados_smp_uf) ufs), agrupadores_por_dados As (Select agrupadores.Data, agrupadores.UF, agrupadores.Pmm, agrupadores.nome_extenso, TO_CHAR(DECODE(dados.Smp7_A, Null, &apos;-&apos;, dados.Smp7_A)) smp7_a, TO_CHAR(DECODE(dados.SMP7_B, Null, &apos;-&apos;, dados.SMP7_B)) smp7_b, TO_CHAR(DECODE(dados.smp7, Null, &apos;-&apos;, dados.smp7)) smp7 From dados_smp_uf dados, agrupadores_smp_uf agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.Pmm = dados.Pmm(+) And agrupadores.UF = dados.UF(+) And agrupadores.nome_extenso = dados.nome_extenso(+) Union All Select a.*, TO_CHAR(DECODE(a.smp7_b, Null, 0, DECODE(a.smp7_b, 0, 0, (DECODE(a.smp7_a, Null, 0, a.smp7_a) * 100 / a.smp7_b)))) From (Select agrupadores.Data, agrupadores.UF, &apos;PMM_TOTAL&apos; PMM, agrupadores.nome_extenso, TO_CHAR(Sum(DECODE(dados.Smp7_A, Null, 0, dados.Smp7_A))) smp7_a, TO_CHAR(Sum(DECODE(dados.SMP7_B, Null, 0, dados.SMP7_B))) smp7_b From dados_smp_uf dados, agrupadores_smp_uf agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.UF = dados.UF(+) And agrupadores.Pmm = dados.Pmm(+) And agrupadores.nome_extenso = dados.nome_extenso(+) Group By agrupadores.Data, agrupadores.UF, agrupadores.nome_extenso) a) Select TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA, agrupadores_por_dados.UF, agrupadores_por_dados.Pmm, NVL(agrupadores_por_dados.nome_extenso, &apos; &apos;) As nome, REPLACE(agrupadores_por_dados.smp7, &apos;.&apos;, &apos;,&apos;) As SMP, agrupadores_por_dados.smp7_a As SMP_A, agrupadores_por_dados.smp7_b As SMP_B From agrupadores_por_dados Order By 2, 1, 7&lt;/SQL&gt;&lt;SubQueries&gt;&lt;SubQuery SQL=&quot;With dados_traducao_sem_vinculo As (Select VIEW_MSC_VINCULO_SMP7.DATA, VIEW_MSC_VINCULO_SMP7.HORA, VIEW_MSC_VINCULO_SMP7.PMM, VIEW_MSC_VINCULO_SMP7.CN, VIEW_MSC_VINCULO_SMP7.MSC, VIEW_MSC_VINCULO_SMP7.OK1_DRP, VIEW_MSC_VINCULO_SMP7.OK1_DRP_TCT, VIEW_MSC_VINCULO_SMP7.OKT, VIEW_MSC_VINCULO_SMP7.OK_ITCP From VIEW_MSC_VINCULO_SMP7 Where VIEW_MSC_VINCULO_SMP7.CODIGO != &apos;NID&apos;), dados_smp_UF As (Select Data, Pmm, UF, Case When SMP7_B = &apos;-&apos; A