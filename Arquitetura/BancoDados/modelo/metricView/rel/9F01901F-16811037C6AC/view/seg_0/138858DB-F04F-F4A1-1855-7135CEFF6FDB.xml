<userDefinedSQL>WITH dados_traducao_sem_vinculo AS&lt;br/&gt;  (SELECT VIEW_MSC_VINCULO_SMP4.DATA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.HORA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.PMM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.CN,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.MSC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OKVM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1_TCT,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1_DRP,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1_DRP_TCT,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.CHAM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.BAC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.NR_VM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.SCV,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.INEX,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.DESL,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.BLO,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.SC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.INV&lt;br/&gt;  FROM VIEW_MSC_VINCULO_SMP4&lt;br/&gt;  WHERE VIEW_MSC_VINCULO_SMP4.CODIGO != &apos;NID&apos;&lt;br/&gt;  ),&lt;br/&gt;  dados_smp_CN AS&lt;br/&gt;  (SELECT Data,&lt;br/&gt;    Pmm,&lt;br/&gt;    CN,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN Smp4_B = &apos;0&apos;&lt;br/&gt;      AND Smp4_A != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN Smp4_B = &apos;-&apos;&lt;br/&gt;      AND Smp4_A  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN Smp4_B != &apos;-&apos;&lt;br/&gt;      AND Smp4_A   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND((Smp4_A * 100 / Smp4_B), 2), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS smp4,&lt;br/&gt;    Smp4_A,&lt;br/&gt;    Smp4_B&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT TRUNC(Tab.DATA)                                                                                                                                                       AS Data,&lt;br/&gt;      pmm.NOME                                                                                                                                                                    AS Pmm,&lt;br/&gt;      Tab.CN                                                                                                                                                                      AS CN,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.OK1, 0)   + NVL(Tab.OKVM, 0) + NVL(Tab.OK1_TCT, 0) + NVL(Tab.OK1_DRP, 0) + NVL(Tab.OK1_DRP_TCT, 0))), &apos;-&apos;)                                          AS Smp4_A,&lt;br/&gt;      NVL(TO_CHAR(SUM((NVL(Tab.CHAM, 0) - NVL(Tab.INV, 0) - (NVL(Tab.SCV, 0) + NVL(Tab.BAC, 0) + NVL(Tab.INEX, 0) + NVL(Tab.DESL, 0) + NVL(Tab.BLO, 0) + NVL(Tab.SC, 0))))), &apos;-&apos;) AS Smp4_B&lt;br/&gt;    FROM VIEW_MSC_CN_ATIVO_SMP4 b,&lt;br/&gt;      dados_traducao_sem_vinculo Tab&lt;br/&gt;    INNER JOIN PMM pmm&lt;br/&gt;    ON Tab.HORA BETWEEN pmm.INICIO AND pmm.FIM&lt;br/&gt;    WHERE Tab.MSC                                        = b.MSC&lt;br/&gt;    AND Tab.CN                                           = b.CN&lt;br/&gt;    AND to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO&lt;br/&gt;    AND b.STATUS                                         = &apos;T&apos;&lt;br/&gt;    GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;      pmm.NOME,&lt;br/&gt;      Tab.CN&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_smp_CN AS&lt;br/&gt;  (SELECT *&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT DISTINCT dados_smp_CN.Data FROM dados_smp_CN&lt;br/&gt;    ) datas,&lt;br/&gt;    (SELECT DISTINCT dados_smp_CN.Pmm FROM dados_smp_CN&lt;br/&gt;    ) pmms,&lt;br/&gt;    (SELECT DISTINCT dados_smp_CN.CN FROM dados_smp_CN&lt;br/&gt;    ) CNs&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_por_dados AS&lt;br/&gt;  (SELECT agrupadores.Data,&lt;br/&gt;    agrupadores.CN,&lt;br/&gt;    agrupadores.Pmm,&lt;br/&gt;    DECODE(dados.Smp4_A, NULL, &apos;-&apos;, dados.Smp4_A) smp4_a,&lt;br/&gt;    DECODE(dados.Smp4_B, NULL, &apos;-&apos;, dados.Smp4_B) smp4_b,&lt;br/&gt;    DECODE(dados.smp4, NULL, &apos;-&apos;, dados.smp4) smp4&lt;br/&gt;  FROM dados_smp_CN dados,&lt;br/&gt;    agrupadores_smp_CN agrupadores&lt;br/&gt;  WHERE agrupadores.Data = dados.Data(+)&lt;br/&gt;  AND agrupadores.Pmm    = dados.Pmm(+)&lt;br/&gt;  AND agrupadores.CN     = dados.CN(+)&lt;br/&gt;  UNION ALL&lt;br/&gt;  SELECT a.*,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN a.smp4_b = &apos;0&apos;&lt;br/&gt;      AND a.smp4_a != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.smp4_b = &apos;-&apos;&lt;br/&gt;      AND a.smp4_a  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN a.smp4_b != &apos;-&apos;&lt;br/&gt;      AND a.smp4_a   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(TO_CHAR(ROUND((a.smp4_a * 100 / a.smp4_b), 2)), &apos;-&apos;)&lt;br/&gt;    END AS smp4&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT agrupadores.Data,&lt;br/&gt;      agrupadores.CN,&lt;br/&gt;      &apos;PMM_TOTAL&apos; PMM,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.Smp4_A, NULL, 0, dados.Smp4_A))) smp4_a,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.Smp4_B, NULL, 0, dados.Smp4_B))) smp4_b&lt;br/&gt;    FROM dados_smp_CN dados,&lt;br/&gt;      agrupadores_smp_CN agrupadores&lt;br/&gt;    WHERE agrupadores.Data = dados.Data(+)&lt;br/&gt;    AND agrupadores.CN     = dados.CN(+)&lt;br/&gt;    AND agrupadores.Pmm    = dados.Pmm(+)&lt;br/&gt;    GROUP BY agrupadores.Data,&lt;br/&gt;      agrupadores.CN,&lt;br/&gt;      &apos;PMM_TOTAL&apos;&lt;br/&gt;    ) a&lt;br/&gt;  )&lt;br/&gt;SELECT TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA,&lt;br/&gt;  TO_CHAR(agrupadores_por_dados.CN) AS CN,&lt;br/&gt;  agrupadores_por_dados.Pmm,&lt;br/&gt;  agrupadores_por_dados.smp4_a AS SMP_A,&lt;br/&gt;  agrupadores_por_dados.smp4_b AS SMP_B,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN agrupadores_por_dados.smp4          != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(agrupadores_por_dados.smp4) &gt; 100&lt;br/&gt;    THEN &apos;100,00&apos;&lt;br/&gt;    ELSE REPLACE(agrupadores_por_dados.smp4, &apos;.&apos;, &apos;,&apos;)&lt;br/&gt;  END AS SMP&lt;br/&gt;FROM agrupadores_por_dados&lt;br/&gt;ORDER BY 2,&lt;br/&gt;  1,&lt;br/&gt;  3</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;With dados_traducao_sem_vinculo As (Select VIEW_MSC_VINCULO_SMP4.DATA, VIEW_MSC_VINCULO_SMP4.HORA, VIEW_MSC_VINCULO_SMP4.PMM, VIEW_MSC_VINCULO_SMP4.CN, VIEW_MSC_VINCULO_SMP4.MSC, VIEW_MSC_VINCULO_SMP4.OK1, VIEW_MSC_VINCULO_SMP4.OKVM, VIEW_MSC_VINCULO_SMP4.OK1_TCT, VIEW_MSC_VINCULO_SMP4.OK1_DRP, VIEW_MSC_VINCULO_SMP4.OK1_DRP_TCT, VIEW_MSC_VINCULO_SMP4.CHAM, VIEW_MSC_VINCULO_SMP4.BAC, VIEW_MSC_VINCULO_SMP4.NR_VM, VIEW_MSC_VINCULO_SMP4.SCV, VIEW_MSC_VINCULO_SMP4.INEX, VIEW_MSC_VINCULO_SMP4.DESL, VIEW_MSC_VINCULO_SMP4.BLO, VIEW_MSC_VINCULO_SMP4.SC, VIEW_MSC_VINCULO_SMP4.INV From VIEW_MSC_VINCULO_SMP4 Where VIEW_MSC_VINCULO_SMP4.CODIGO != &apos;NID&apos;), dados_smp_CN As (Select Data, Pmm, CN, Case When Smp4_B = &apos;0&apos; And Smp4_A != &apos;-&apos; Then &apos;0,00&apos; When Smp4_B = &apos;-&apos; And Smp4_A = &apos;-&apos; Then &apos;-&apos; When Smp4_B != &apos;-&apos; And Smp4_A = &apos;-&apos; Then &apos;0,00&apos; Else NVL(trim(TO_CHAR(ROUND((Smp4_A * 100 / Smp4_B), 2), &apos;999990D99&apos;)), &apos;-&apos;) End As smp4, Smp4_A, Smp4_B From (Select TRUNC(Tab.DATA) As Data, pmm.NOME As Pmm, Tab.CN As CN, NVL(TO_CHAR(Sum(NVL(Tab.OK1, 0) + NVL(Tab.OKVM, 0) + NVL(Tab.OK1_TCT, 0) + NVL(Tab.OK1_DRP, 0) + NVL(Tab.OK1_DRP_TCT, 0))), &apos;-&apos;) As Smp4_A, NVL(TO_CHAR(Sum((NVL(Tab.CHAM, 0) - NVL(Tab.INV, 0) - (NVL(Tab.SCV, 0) + NVL(Tab.BAC, 0) + NVL(Tab.INEX, 0) + NVL(Tab.DESL, 0) + NVL(Tab.BLO, 0) + NVL(Tab.SC, 0))))), &apos;-&apos;) As Smp4_B From VIEW_MSC_CN_ATIVO_SMP4 b, dados_traducao_sem_vinculo Tab Inner Join PMM pmm On Tab.HORA Between pmm.INICIO And pmm.FIM Where Tab.MSC = b.MSC And Tab.CN = b.CN And to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO And b.STATUS = &apos;T&apos; Group By TRUNC(Tab.DATA), pmm.NOME, Tab.CN)), agrupadores_smp_CN As (Select * From (Select Distinct dados_smp_CN.Data From dados_smp_CN) datas, (Select Distinct dados_smp_CN.Pmm From dados_smp_CN) pmms, (Select Distinct dados_smp_CN.CN From dados_smp_CN) CNs), agrupadores_por_dados As (Select agrupadores.Data, agrupadores.CN, agrupadores.Pmm, DECODE(dados.Smp4_A, Null, &apos;-&apos;, dados.Smp4_A) smp4_a, DECODE(dados.Smp4_B, Null, &apos;-&apos;, dados.Smp4_B) smp4_b, DECODE(dados.smp4, Null, &apos;-&apos;, dados.smp4) smp4 From dados_smp_CN dados, agrupadores_smp_CN agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.Pmm = dados.Pmm(+) And agrupadores.CN = dados.CN(+) Union All Select a.*, Case When a.smp4_b = &apos;0&apos; And a.smp4_a != &apos;-&apos; Then &apos;0,00&apos; When a.smp4_b = &apos;-&apos; And a.smp4_a = &apos;-&apos; Then &apos;-&apos; When a.smp4_b != &apos;-&apos; And a.smp4_a = &apos;-&apos; Then &apos;0,00&apos; Else NVL(TO_CHAR(ROUND((a.smp4_a * 100 / a.smp4_b), 2)), &apos;-&apos;) End As smp4 From (Select agrupadores.Data, agrupadores.CN, &apos;PMM_TOTAL&apos; PMM, TO_CHAR(Sum(DECODE(dados.Smp4_A, Null, 0, dados.Smp4_A))) smp4_a, TO_CHAR(Sum(DECODE(dados.Smp4_B, Null, 0, dados.Smp4_B))) smp4_b From dados_smp_CN dados, agrupadores_smp_CN agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.CN = dados.CN(+) And agrupadores.Pmm = dados.Pmm(+) Group By agrupadores.Data, agrupadores.CN, &apos;PMM_TOTAL&apos;) a) Select TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA, TO_CHAR(agrupadores_por_dados.CN) As CN, agrupadores_por_dados.Pmm, agrupadores_por_dados.smp4_a As SMP_A, agrupadores_por_dados.smp4_b As SMP_B, Case When agrupadores_por_dados.smp4 != &apos;-&apos; And TO_NUMBER(agrupadores_por_dados.smp4) &gt; 100 Then &apos;100,00&apos; Else REPLACE(agrupadores_por_dados.smp4, &apos;.&apos;, &apos;,&apos;) End As SMP From agrupadores_por_dados Order By 2, 1, 3&lt;/SQL&gt;&lt;SubQueries&gt;&lt;SubQuery SQL=&quot;With dados_traducao_sem_vinculo As (Select VIEW_MSC_VINCULO_SMP4.DATA, VIEW_MSC_VINCULO_SMP4.HORA, VIEW_MSC_VIN