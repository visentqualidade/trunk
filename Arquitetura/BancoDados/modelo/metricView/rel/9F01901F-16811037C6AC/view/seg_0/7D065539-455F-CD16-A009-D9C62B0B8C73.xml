<userDefinedSQL>WITH dados_traducao_sem_vinculo AS&lt;br/&gt;  (SELECT VIEW_MSC_VINCULO_SMP4.DATA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.HORA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.PMM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.CN,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.MSC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OKVM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1_TCT,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1_DRP,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.OK1_DRP_TCT,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.CHAM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.BAC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.NR_VM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.SCV,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.INEX,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.DESL,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.BLO,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.SC,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP4.INV&lt;br/&gt;  FROM VIEW_MSC_VINCULO_SMP4&lt;br/&gt;  WHERE VIEW_MSC_VINCULO_SMP4.CODIGO != &apos;NID&apos;&lt;br/&gt;  ),&lt;br/&gt;  dados_smp_UF AS&lt;br/&gt;  (SELECT Data,&lt;br/&gt;    Pmm,&lt;br/&gt;    UF,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN Smp4_B = &apos;0&apos;&lt;br/&gt;      AND Smp4_A != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN Smp4_B = &apos;-&apos;&lt;br/&gt;      AND Smp4_A  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN Smp4_B != &apos;-&apos;&lt;br/&gt;      AND Smp4_A   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND((Smp4_A * 100 / Smp4_B), 2), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS smp4,&lt;br/&gt;    Smp4_A,&lt;br/&gt;    Smp4_B,&lt;br/&gt;    nome_extenso&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT TRUNC(Tab.DATA) AS Data,&lt;br/&gt;      pmm.NOME              AS Pmm,&lt;br/&gt;      uf.nome               AS UF,&lt;br/&gt;      uf.nome_extenso,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.OK1, 0)   + NVL(Tab.OKVM, 0) + NVL(Tab.OK1_TCT, 0) + NVL(Tab.OK1_DRP, 0) + NVL(Tab.OK1_DRP_TCT, 0))), &apos;-&apos;)                                          AS Smp4_A,&lt;br/&gt;      NVL(TO_CHAR(SUM((NVL(Tab.CHAM, 0) - NVL(Tab.INV, 0) - (NVL(Tab.SCV, 0) + NVL(Tab.BAC, 0) + NVL(Tab.INEX, 0) + NVL(Tab.DESL, 0) + NVL(Tab.BLO, 0) + NVL(Tab.SC, 0))))), &apos;-&apos;) AS Smp4_B&lt;br/&gt;    FROM VIEW_MSC_CN_ATIVO_SMP4 b,&lt;br/&gt;      dados_traducao_sem_vinculo Tab&lt;br/&gt;    INNER JOIN PMM pmm&lt;br/&gt;    ON Tab.HORA BETWEEN pmm.INICIO AND pmm.FIM&lt;br/&gt;    INNER JOIN CN cn&lt;br/&gt;    ON cn.nome = Tab.CN&lt;br/&gt;    INNER JOIN UF uf&lt;br/&gt;    ON uf.id_uf                                          = cn.id_uf&lt;br/&gt;    WHERE Tab.MSC                                        = b.MSC&lt;br/&gt;    AND Tab.CN                                           = b.CN&lt;br/&gt;    AND to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO&lt;br/&gt;    AND b.STATUS                                         = &apos;T&apos;&lt;br/&gt;    GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;      pmm.NOME,&lt;br/&gt;      uf.nome,&lt;br/&gt;      uf.nome_extenso&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_smp_uf AS&lt;br/&gt;  (SELECT *&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.Data FROM dados_smp_uf&lt;br/&gt;    ) datas,&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.Pmm FROM dados_smp_uf&lt;br/&gt;    ) pmms,&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.UF, dados_smp_uf.nome_extenso FROM dados_smp_uf&lt;br/&gt;    ) ufs&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_por_dados AS&lt;br/&gt;  (SELECT agrupadores.Data,&lt;br/&gt;    agrupadores.UF,&lt;br/&gt;    agrupadores.Pmm,&lt;br/&gt;    agrupadores.nome_extenso,&lt;br/&gt;    TO_CHAR(DECODE(dados.Smp4_A, NULL, 0, dados.Smp4_A)) smp4_a,&lt;br/&gt;    TO_CHAR(DECODE(dados.Smp4_B, NULL, 0, dados.Smp4_B)) smp4_b,&lt;br/&gt;    TO_CHAR(DECODE(dados.smp4, NULL, 0, dados.smp4)) smp4&lt;br/&gt;  FROM dados_smp_uf dados,&lt;br/&gt;    agrupadores_smp_uf agrupadores&lt;br/&gt;  WHERE agrupadores.Data       = dados.Data(+)&lt;br/&gt;  AND agrupadores.Pmm          = dados.Pmm(+)&lt;br/&gt;  AND agrupadores.UF           = dados.UF(+)&lt;br/&gt;  AND agrupadores.nome_extenso = dados.nome_extenso(+)&lt;br/&gt;  UNION ALL&lt;br/&gt;  SELECT a.*,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN a.smp4_b = &apos;0&apos;&lt;br/&gt;      AND a.smp4_a != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.smp4_b = &apos;-&apos;&lt;br/&gt;      AND a.smp4_a  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN a.smp4_b != &apos;-&apos;&lt;br/&gt;      AND a.smp4_a   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(TO_CHAR(ROUND((a.smp4_a * 100 / a.smp4_b), 2)), &apos;-&apos;)&lt;br/&gt;    END AS smp4&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT agrupadores.Data,&lt;br/&gt;      agrupadores.UF,&lt;br/&gt;      &apos;PMM_TOTAL&apos; PMM,&lt;br/&gt;      agrupadores.nome_extenso,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.Smp4_A, NULL, 0, dados.Smp4_A))) smp4_a,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.Smp4_B, NULL, 0, dados.Smp4_B))) smp4_b&lt;br/&gt;    FROM dados_smp_uf dados,&lt;br/&gt;      agrupadores_smp_uf agrupadores&lt;br/&gt;    WHERE agrupadores.Data       = dados.Data(+)&lt;br/&gt;    AND agrupadores.UF           = dados.UF(+)&lt;br/&gt;    AND agrupadores.Pmm          = dados.Pmm(+)&lt;br/&gt;    AND agrupadores.nome_extenso = dados.nome_extenso(+)&lt;br/&gt;    GROUP BY agrupadores.Data,&lt;br/&gt;      agrupadores.UF,&lt;br/&gt;      &apos;PMM_TOTAL&apos;,&lt;br/&gt;      agrupadores.nome_extenso&lt;br/&gt;    ) a&lt;br/&gt;  )&lt;br/&gt;SELECT TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA,&lt;br/&gt;  agrupadores_por_dados.UF,&lt;br/&gt;  agrupadores_por_dados.Pmm,&lt;br/&gt;  NVL(agrupadores_por_dados.nome_extenso, &apos; &apos;) AS nome,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN agrupadores_por_dados.smp4          != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(agrupadores_por_dados.smp4) &gt; 100&lt;br/&gt;    THEN &apos;100,00&apos;&lt;br/&gt;    ELSE REPLACE(agrupadores_por_dados.smp4, &apos;.&apos;, &apos;,&apos;)&lt;br/&gt;  END                          AS SMP,&lt;br/&gt;  agrupadores_por_dados.smp4_a AS SMP_A,&lt;br/&gt;  agrupadores_por_dados.smp4_b AS SMP_B&lt;br/&gt;FROM agrupadores_por_dados&lt;br/&gt;ORDER BY 2,&lt;br/&gt;  1,&lt;br/&gt;  3</userDefinedSQL>
