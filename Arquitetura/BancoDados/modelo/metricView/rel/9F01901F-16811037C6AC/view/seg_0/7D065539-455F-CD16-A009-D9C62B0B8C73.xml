UNC(Tab.DATA), pmm.NOME, uf.nome, uf.nome_extenso)), agrupadores_smp_uf As (Select * From (Select Distinct dados_smp_uf.Data From dados_smp_uf) datas, (Select Distinct dados_smp_uf.Pmm From dados_smp_uf) pmms, (Select Distinct dados_smp_uf.UF, dados_smp_uf.nome_extenso From dados_smp_uf) ufs), agrupadores_por_dados As (Select agrupadores.Data, agrupadores.UF, agrupadores.Pmm, agrupadores.nome_extenso, TO_CHAR(DECODE(dados.Smp4_A, Null, 0, dados.Smp4_A)) smp4_a, TO_CHAR(DECODE(dados.Smp4_B, Null, 0, dados.Smp4_B)) smp4_b, TO_CHAR(DECODE(dados.smp4, Null, 0, dados.smp4)) smp4 From dados_smp_uf dados, agrupadores_smp_uf agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.Pmm = dados.Pmm(+) And agrupadores.UF = dados.UF(+) And agrupadores.nome_extenso = dados.nome_extenso(+) Union All Select a.*, Case When a.smp4_b = &apos;0&apos; And a.smp4_a != &apos;-&apos; Then &apos;0,00&apos; When a.smp4_b = &apos;-&apos; And a.smp4_a = &apos;-&apos; Then &apos;-&apos; When a.smp4_b != &apos;-&apos; And a.smp4_a = &apos;-&apos; Then &apos;0,00&apos; Else NVL(TO_CHAR(ROUND((a.smp4_a * 100 / a.smp4_b), 2)), &apos;-&apos;) End As smp4 From (Select agrupadores.Data, agrupadores.UF, &apos;PMM_TOTAL&apos; PMM, agrupadores.nome_extenso, TO_CHAR(Sum(DECODE(dados.Smp4_A, Null, 0, dados.Smp4_A))) smp4_a, TO_CHAR(Sum(DECODE(dados.Smp4_B, Null, 0, dados.Smp4_B))) smp4_b From dados_smp_uf dados, agrupadores_smp_uf agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.UF = dados.UF(+) And agrupadores.Pmm = dados.Pmm(+) And agrupadores.nome_extenso = dados.nome_extenso(+) Group By agrupadores.Data, agrupadores.UF, &apos;PMM_TOTAL&apos;, agrupadores.nome_extenso) a) Select TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA, agrupadores_por_dados.UF, agrupadores_por_dados.Pmm, NVL(agrupadores_por_dados.nome_extenso, &apos; &apos;) As nome, Case When agrupadores_por_dados.smp4 != &apos;-&apos; And TO_NUMBER(agrupadores_por_dados.smp4) &gt; 100 Then &apos;10