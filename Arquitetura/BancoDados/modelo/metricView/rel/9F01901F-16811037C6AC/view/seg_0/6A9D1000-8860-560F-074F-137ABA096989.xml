<afterCreate>CREATE OR REPLACE FORCE VIEW &quot;EASYVIEW&quot;.&quot;VIEW_SMP9_PAINEL_ACOMP&quot; (&quot;DATA&quot;, &quot;SMP9&quot;, &quot;CN&quot;, &quot;PMM&quot;) AS &lt;br/&gt;  WITH Dias AS&lt;br/&gt;  (SELECT UNIQUE TO_CHAR(Dt.DATA, &apos;dd&apos;) AS Data&lt;br/&gt;  FROM CDRVIEW_SMP9 Dt&lt;br/&gt;  WHERE TO_CHAR(Dt.DATA, &apos;dd&apos;) &gt;= 1&lt;br/&gt;  AND TO_CHAR(Dt.DATA, &apos;dd&apos;)   &lt;= 10&lt;br/&gt;  ),&lt;br/&gt;  MesAno AS&lt;br/&gt;  (SELECT UNIQUE TO_CHAR(Dt.DATA, &apos;mm/yyyy&apos;) AS Data FROM CDRVIEW_SMP9 Dt&lt;br/&gt;  ),&lt;br/&gt;  Dia_X_Mes_X_Ano AS&lt;br/&gt;  (SELECT Concat(Concat(Dias.Data, &apos;/&apos;), Mesano.Data) AS data&lt;br/&gt;  FROM Dias Dias,&lt;br/&gt;    Mesano Mesano&lt;br/&gt;  ),&lt;br/&gt;  dados_traducao_sem_vinculo as &lt;br/&gt;  (select DATA, CENTRAL, CN, PMM, REGIONAL, UF, SMP9_A, SMP9_B  from view_msc_vinculo_smp9 where codigo != &apos;NID&apos;&lt;br/&gt;  ),&lt;br/&gt;  Smps_dados AS&lt;br/&gt;  (SELECT Data  AS DATA,&lt;br/&gt;    TO_CHAR(CN) AS CN,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN SMP9_B            = &apos;0&apos;&lt;br/&gt;      AND SMP9_A            != &apos;-&apos;&lt;br/&gt;      AND TO_NUMBER(SMP9_A) &gt;= 0&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN (SMP9_B = &apos;-&apos;&lt;br/&gt;      AND SMP9_A   = &apos;-&apos;)&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN SMP9_B != &apos;0&apos;&lt;br/&gt;      AND SMP9_B  != &apos;-&apos;&lt;br/&gt;      AND SMP9_A   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND(((SMP9_A / SMP9_B) * 100),2),&apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS SMP9&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT TRUNC(Tab.DATA) AS Data,&lt;br/&gt;      Tab.CN,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(SMP9_A, 0))), &apos;-&apos;) AS SMP9_A,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(SMP9_B, 0))), &apos;-&apos;) AS SMP9_B&lt;br/&gt;    FROM &lt;br/&gt;    view_msc_cn_ativo_smp9 b,&lt;br/&gt;    dados_traducao_sem_vinculo Tab&lt;br/&gt;    WHERE TO_CHAR(Tab.DATA, &apos;dd&apos;) IN&lt;br/&gt;      (SELECT Dias.Data FROM Dias)&lt;br/&gt;      and tab.central = b.msc and tab.cn = b.cn&lt;br/&gt;      and to_date(to_char(tab.data,&apos;MM/yyyy&apos;),&apos;MM/yyyy&apos;) = b.periodo&lt;br/&gt;      and b.status = &apos;T&apos;&lt;br/&gt;    GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;      Tab.CN&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  Dias_X_Cn_X_Pmm AS&lt;br/&gt;  (SELECT to_date(D.data, &apos;dd/mm/yyyy&apos;) AS Data,&lt;br/&gt;    Cn.nome&lt;br/&gt;  FROM CN Cn,&lt;br/&gt;    Dia_X_Mes_X_Ano D&lt;br/&gt;  ),&lt;br/&gt;  Dias_X_Cn_X_Pmm_X_Smp AS&lt;br/&gt;  (SELECT NVL(TRUNC(Smpdados.DATA), Diascnpmm.Data) AS Data,&lt;br/&gt;    NVL(TO_CHAR(Smpdados.SMP9), &apos;-&apos;)                AS SMP9,&lt;br/&gt;    Diascnpmm.nome                                  AS Cn&lt;br/&gt;  FROM Dias_X_Cn_X_Pmm Diascnpmm,&lt;br/&gt;    Smps_Dados Smpdados&lt;br/&gt;  WHERE TRUNC(Diascnpmm.Data) = TRUNC(Smpdados.DATA(+))&lt;br/&gt;  AND Diascnpmm.nome          = Smpdados.CN(+)&lt;br/&gt;  )&lt;br/&gt;SELECT TO_CHAR(Dias_X_Cn_X_Pmm_X_Smp.Data, &apos;DD/MM/YYYY&apos;) AS DATA,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN (Dias_X_Cn_X_Pmm_X_Smp.SMP9         != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(Dias_X_Cn_X_Pmm_X_Smp.SMP9) &gt; 100)&lt;br/&gt;    THEN &apos;100,00&apos;&lt;br/&gt;    ELSE replace(Dias_X_Cn_X_Pmm_X_Smp.SMP9,&apos;.&apos;,&apos;,&apos;)&lt;br/&gt;  END                               AS SMP9,&lt;br/&gt;  TO_CHAR(Dias_X_Cn_X_Pmm_X_Smp.Cn) AS CN,&lt;br/&gt;  TO_CHAR(&apos;PMM&apos;)                    AS PMM&lt;br/&gt;FROM Dias_X_Cn_X_Pmm_X_Smp&lt;br/&gt;ORDER BY TO_CHAR(Dias_X_Cn_X_Pmm_X_Smp.Cn),&lt;br/&gt;  TO_CHAR(Dias_X_Cn_X_Pmm_X_Smp.Data, &apos;DD/MM/YYYY&apos;);</afterCreate>
