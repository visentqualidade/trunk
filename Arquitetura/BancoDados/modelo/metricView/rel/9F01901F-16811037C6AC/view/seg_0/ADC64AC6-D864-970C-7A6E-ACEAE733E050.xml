<userDefinedSQL>WITH dados_traducao_sem_vinculo AS&lt;br/&gt;  (SELECT VIEW_MSC_VINCULO_SMP5.DATA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.CENTRAL,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.CN,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.PMM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.REGIONAL,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_A,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_B,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_C,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_D,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.UF&lt;br/&gt;  FROM VIEW_MSC_VINCULO_SMP5&lt;br/&gt;  WHERE VIEW_MSC_VINCULO_SMP5.CODIGO != &apos;NID&apos;&lt;br/&gt;  ),&lt;br/&gt;  dados_smp_UF AS&lt;br/&gt;  (SELECT Data,&lt;br/&gt;    Pmm,&lt;br/&gt;    UF,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN SMP5_B = &apos;0&apos;&lt;br/&gt;      AND SMP5_A != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP5_B = &apos;-&apos;&lt;br/&gt;      AND SMP5_A  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN SMP5_B != &apos;-&apos;&lt;br/&gt;      AND SMP5_A   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP5_D = &apos;0&apos;&lt;br/&gt;      AND SMP5_C != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP5_D = &apos;-&apos;&lt;br/&gt;      AND SMP5_C  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN SMP5_D != &apos;-&apos;&lt;br/&gt;      AND SMP5_C   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND(((SMP5_A / SMP5_B) * (SMP5_C / SMP5_D) * 100)), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS SMP5,&lt;br/&gt;    SMP5_A,&lt;br/&gt;    SMP5_B,&lt;br/&gt;    SMP5_C,&lt;br/&gt;    SMP5_D,&lt;br/&gt;    nome_extenso&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT TRUNC(Tab.DATA) AS Data,&lt;br/&gt;      pmm.NOME              AS Pmm,&lt;br/&gt;      uf.nome               AS UF,&lt;br/&gt;      uf.nome_extenso,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(SMP5_A, 0))), &apos;-&apos;) AS SMP5_A,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(SMP5_b, 0))), &apos;-&apos;) AS SMP5_B,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(SMP5_c, 0))), &apos;-&apos;) AS SMP5_C,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(SMP5_d, 0))), &apos;-&apos;) AS SMP5_D&lt;br/&gt;    FROM VIEW_MSC_CN_ATIVO_SMP5 b,&lt;br/&gt;      dados_traducao_sem_vinculo Tab&lt;br/&gt;    INNER JOIN PMM pmm&lt;br/&gt;    ON Tab.PMM = pmm.NOME&lt;br/&gt;    INNER JOIN CN cn&lt;br/&gt;    ON cn.nome = Tab.CN&lt;br/&gt;    INNER JOIN UF uf&lt;br/&gt;    ON uf.id_uf                                          = cn.id_uf&lt;br/&gt;    WHERE Tab.CENTRAL                                    = b.MSC&lt;br/&gt;    AND Tab.CN                                           = b.CN&lt;br/&gt;    AND to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO&lt;br/&gt;    AND b.STATUS                                         = &apos;T&apos;&lt;br/&gt;    GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;      pmm.NOME,&lt;br/&gt;      uf.nome,&lt;br/&gt;      uf.nome_extenso&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_smp_uf AS&lt;br/&gt;  (SELECT *&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.Data FROM dados_smp_uf&lt;br/&gt;    ) datas,&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.Pmm FROM dados_smp_uf&lt;br/&gt;    ) pmms,&lt;br/&gt;    (SELECT DISTINCT dados_smp_uf.UF, dados_smp_uf.nome_extenso FROM dados_smp_uf&lt;br/&gt;    ) ufs&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_por_dados AS&lt;br/&gt;  (SELECT agrupadores.Data,&lt;br/&gt;    agrupadores.UF,&lt;br/&gt;    agrupadores.Pmm,&lt;br/&gt;    agrupadores.nome_extenso,&lt;br/&gt;    TO_CHAR(DECODE(dados.SMP5_A, NULL, 0, dados.SMP5_A)) SMP5_a,&lt;br/&gt;    TO_CHAR(DECODE(dados.SMP5_B, NULL, 0, dados.SMP5_B)) SMP5_b,&lt;br/&gt;    TO_CHAR(DECODE(dados.SMP5_C, NULL, 0, dados.SMP5_C)) SMP5_c,&lt;br/&gt;    TO_CHAR(DECODE(dados.SMP5_D, NULL, 0, dados.SMP5_D)) SMP5_d,&lt;br/&gt;    TO_CHAR(DECODE(dados.SMP5, NULL, 0, dados.SMP5)) SMP5&lt;br/&gt;  FROM dados_smp_uf dados,&lt;br/&gt;    agrupadores_smp_uf agrupadores&lt;br/&gt;  WHERE agrupadores.Data       = dados.Data(+)&lt;br/&gt;  AND agrupadores.Pmm          = dados.Pmm(+)&lt;br/&gt;  AND agrupadores.UF           = dados.UF(+)&lt;br/&gt;  AND agrupadores.nome_extenso = dados.nome_extenso(+)&lt;br/&gt;  UNION ALL&lt;br/&gt;  SELECT a.*,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN a.SMP5_b = &apos;0&apos;&lt;br/&gt;      AND a.SMP5_a != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.SMP5_b = &apos;-&apos;&lt;br/&gt;      AND a.SMP5_a  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN a.SMP5_b != &apos;-&apos;&lt;br/&gt;      AND a.SMP5_a   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.SMP5_d = &apos;0&apos;&lt;br/&gt;      AND a.SMP5_c != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.SMP5_d = &apos;-&apos;&lt;br/&gt;      AND a.SMP5_c  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN a.SMP5_d != &apos;-&apos;&lt;br/&gt;      AND a.SMP5_c   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(TO_CHAR(ROUND(((a.SMP5_a / a.SMP5_b) * (a.SMP5_c / a.SMP5_d) * 100), 2)), &apos;-&apos;)&lt;br/&gt;    END AS SMP5&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT agrupadores.Data,&lt;br/&gt;      agrupadores.UF,&lt;br/&gt;      &apos;PMM_TOTAL&apos; PMM,&lt;br/&gt;      agrupadores.nome_extenso,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_A, NULL, 0, dados.SMP5_A))) SMP5_a,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_B, NULL, 0, dados.SMP5_B))) SMP5_b,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_C, NULL, 0, dados.SMP5_C))) SMP5_c,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_D, NULL, 0, dados.SMP5_D))) SMP5_d&lt;br/&gt;    FROM dados_smp_uf dados,&lt;br/&gt;      agrupadores_smp_uf agrupadores&lt;br/&gt;    WHERE agrupadores.Data       = dados.Data(+)&lt;br/&gt;    AND agrupadores.UF           = dados.UF(+)&lt;br/&gt;    AND agrupadores.Pmm          = dados.Pmm(+)&lt;br/&gt;    AND agrupadores.nome_extenso = dados.nome_extenso(+)&lt;br/&gt;    GROUP BY agrupadores.Data,&lt;br/&gt;      agrupadores.UF,&lt;br/&gt;      &apos;PMM_TOTAL&apos;,&lt;br/&gt;      agrupadores.nome_extenso&lt;br/&gt;    ) a&lt;br/&gt;  )&lt;br/&gt;SELECT TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA,&lt;br/&gt;  agrupadores_por_dados.UF,&lt;br/&gt;  agrupadores_por_dados.Pmm,&lt;br/&gt;  NVL(agrupadores_por_dados.nome_extenso, &apos; &apos;) AS nome,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN agrupadores_por_dados.SMP5          != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(agrupadores_por_dados.SMP5) &gt; 100&lt;br/&gt;    THEN &apos;100,00&apos;&lt;br/&gt;    ELSE REPLACE(agrupadores_por_dados.SMP5, &apos;.&apos;, &apos;,&apos;)&lt;br/&gt;  END                          AS SMP,&lt;br/&gt;  agrupadores_por_dados.SMP5_a AS SMP_A,&lt;br/&gt;  agrupadores_por_dados.SMP5_b AS SMP_B,&lt;br/&gt;  agrupadores_por_dados.SMP5_a AS SMP_C,&lt;br/&gt;  agrupadores_por_dados.SMP5_b AS SMP_D&lt;br/&gt;FROM agrupadores_por_dados&lt;br/&gt;ORDER BY 2,&lt;br/&gt;  1,&lt;br/&gt;  3</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;With dados_traducao_sem_vinculo As (Select VIEW_MSC_VINCULO_SMP5.DATA, VIEW_MSC_VINCULO_SMP5.CENTRAL, VIEW_MSC_VINCULO_SMP5.CN, VIEW_MSC_VINCULO_SMP5.PMM, VIEW_MSC_VINCULO_SMP5.REGIONAL, VIEW_MSC_VINCULO_SMP5.SMP5_A, VIEW_MSC_VINCULO_SMP5.SMP5_B, VIEW_MSC_VINCULO_SMP5.SMP5_C, VIEW_MSC_VINCULO_SMP5.SMP5_D, VIEW_MSC_VINCULO_SMP5.UF From VIEW_MSC_VINCULO_SMP5 Where VIEW_MSC_VINCULO_SMP5.CODIGO != &apos;NID&apos;), dados_smp_UF As (Select Data, Pmm, UF, Case When SMP5_B = &apos;0&apos; And SMP5_A != &apos;-&apos; Then &apos;0,00&apos; When SMP5_B = &apos;-&apos; And SMP5_A = &apos;-&apos; Then &apos;-&apos; When SMP5_B != &apos;-&apos; And SMP5_A = &apos;-&apos; Then &apos;0,00&apos; When SMP5_D = &apos;0&apos; And SMP5_C != &apos;-&apos; Then &apos;0,00&apos; When SMP5_D = &apos;-&apos; And SMP5_C = &apos;-&apos; Then &apos;-&apos; When SMP5_D != &apos;-&apos; And SMP5_C = &apos;-&apos; Then &apos;0,00&apos; Else NVL(trim(TO_CHAR(ROUND(((SMP5_A / SMP5_B) * (SMP5_C / SMP5_D) * 100)), &apos;999990D99&apos;)), &apos;-&apos;) End As SMP5, SMP5_A, SMP5_B, SMP5_C, SMP5_D, nome_extenso From (Select TRUNC(Tab.DATA) As Data, pmm.NOME As Pmm, uf.nome As UF, uf.nome_extenso, NVL(TO_CHAR(Sum(NVL(SMP5_A, 0))), &apos;-&apos;) As SMP5_A, NVL(TO_CHAR(Sum(NVL(SMP5_b, 0))), &apos;-&apos;) As SMP5_B, NVL(TO_CHAR(Sum(NVL(SMP5_c, 0))), &apos;-&apos;) As SMP5_C, NVL(TO_CHAR(Sum(NVL(SMP5_d, 0))), &apos;-&apos;) As SMP5_D From VIEW_MSC_CN_ATIVO_SMP5 b, dados_traducao_sem_vinculo Tab Inner Join PMM pmm On Tab.PMM = pmm.NOME Inner Join CN cn On cn.nome = Tab.CN Inner Join UF uf On uf.id_uf = cn.id_uf Where Tab.CENTRAL = b.MSC And Tab.CN = b.CN And to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO And b.STATUS = &apos;T&apos; Group By TRUNC(Tab.DATA), pmm.NOME, uf.nome, uf.nome_extenso)), agrupadores_smp_uf As (Select * From (Select Di