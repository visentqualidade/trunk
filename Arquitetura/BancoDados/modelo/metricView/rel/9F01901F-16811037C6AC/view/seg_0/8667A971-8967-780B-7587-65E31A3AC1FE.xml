<userDefinedSQL>WITH dados_traducao_sem_vinculo AS&lt;br/&gt;  (SELECT VIEW_MSC_VINCULO_SMP5.DATA,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.CENTRAL,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.CN,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.PMM,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.REGIONAL,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_A,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_B,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_C,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.SMP5_D,&lt;br/&gt;    VIEW_MSC_VINCULO_SMP5.UF&lt;br/&gt;  FROM VIEW_MSC_VINCULO_SMP5&lt;br/&gt;  WHERE VIEW_MSC_VINCULO_SMP5.CODIGO != &apos;NID&apos;&lt;br/&gt;  ),&lt;br/&gt;  dados_smp_CN AS&lt;br/&gt;  (SELECT Data,&lt;br/&gt;    Pmm,&lt;br/&gt;    CN,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN SMP5_B = &apos;0&apos;&lt;br/&gt;      AND SMP5_A != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP5_B = &apos;-&apos;&lt;br/&gt;      AND SMP5_A  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN SMP5_B != &apos;-&apos;&lt;br/&gt;      AND SMP5_A   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP5_D = &apos;0&apos;&lt;br/&gt;      AND SMP5_C != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN SMP5_D = &apos;-&apos;&lt;br/&gt;      AND SMP5_C  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN SMP5_D != &apos;-&apos;&lt;br/&gt;      AND SMP5_C   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND(((SMP5_A / SMP5_B) * (SMP5_C / SMP5_D) * 100), 2), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS SMP5,&lt;br/&gt;    SMP5_A,&lt;br/&gt;    SMP5_B,&lt;br/&gt;    SMP5_C,&lt;br/&gt;    SMP5_D&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT TRUNC(Tab.DATA)                      AS Data,&lt;br/&gt;      pmm.NOME                                   AS Pmm,&lt;br/&gt;      Tab.CN                                     AS CN,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.SMP5_A, 0))), &apos;-&apos;) AS SMP5_A,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.SMP5_B, 0))), &apos;-&apos;) AS SMP5_B,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.SMP5_C, 0))), &apos;-&apos;) AS SMP5_C,&lt;br/&gt;      NVL(TO_CHAR(SUM(NVL(Tab.SMP5_D, 0))), &apos;-&apos;) AS SMP5_D&lt;br/&gt;    FROM VIEW_MSC_CN_ATIVO_SMP5 b,&lt;br/&gt;      dados_traducao_sem_vinculo Tab&lt;br/&gt;    INNER JOIN PMM pmm&lt;br/&gt;    ON Tab.PMM                                           = pmm.NOME&lt;br/&gt;    WHERE Tab.CENTRAL                                    = b.MSC&lt;br/&gt;    AND Tab.CN                                           = b.CN&lt;br/&gt;    AND to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO&lt;br/&gt;    AND b.STATUS                                         = &apos;T&apos;&lt;br/&gt;    GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;      pmm.NOME,&lt;br/&gt;      Tab.CN&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_smp_CN AS&lt;br/&gt;  (SELECT *&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT DISTINCT dados_smp_CN.Data FROM dados_smp_CN&lt;br/&gt;    ) datas,&lt;br/&gt;    (SELECT DISTINCT dados_smp_CN.Pmm FROM dados_smp_CN&lt;br/&gt;    ) pmms,&lt;br/&gt;    (SELECT DISTINCT dados_smp_CN.CN FROM dados_smp_CN&lt;br/&gt;    ) CNs&lt;br/&gt;  ),&lt;br/&gt;  agrupadores_por_dados AS&lt;br/&gt;  (SELECT agrupadores.Data,&lt;br/&gt;    agrupadores.CN,&lt;br/&gt;    agrupadores.Pmm,&lt;br/&gt;    DECODE(dados.SMP5_A, NULL, &apos;-&apos;, dados.SMP5_A) SMP5_a,&lt;br/&gt;    DECODE(dados.SMP5_B, NULL, &apos;-&apos;, dados.SMP5_B) SMP5_b,&lt;br/&gt;    DECODE(dados.SMP5_C, NULL, &apos;-&apos;, dados.SMP5_C) SMP5_c,&lt;br/&gt;    DECODE(dados.SMP5_D, NULL, &apos;-&apos;, dados.SMP5_D) SMP5_d,&lt;br/&gt;    DECODE(dados.SMP5, NULL, &apos;-&apos;, dados.SMP5) SMP5&lt;br/&gt;  FROM dados_smp_CN dados,&lt;br/&gt;    agrupadores_smp_CN agrupadores&lt;br/&gt;  WHERE agrupadores.Data = dados.Data(+)&lt;br/&gt;  AND agrupadores.Pmm    = dados.Pmm(+)&lt;br/&gt;  AND agrupadores.CN     = dados.CN(+)&lt;br/&gt;  UNION ALL&lt;br/&gt;  SELECT a.*,&lt;br/&gt;    CASE&lt;br/&gt;      WHEN a.SMP5_b = &apos;0&apos;&lt;br/&gt;      AND a.SMP5_a != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.SMP5_b = &apos;-&apos;&lt;br/&gt;      AND a.SMP5_a  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN a.SMP5_b != &apos;-&apos;&lt;br/&gt;      AND a.SMP5_a   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.SMP5_d = &apos;0&apos;&lt;br/&gt;      AND a.SMP5_c != &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      WHEN a.SMP5_d = &apos;-&apos;&lt;br/&gt;      AND a.SMP5_c  = &apos;-&apos;&lt;br/&gt;      THEN &apos;-&apos;&lt;br/&gt;      WHEN a.SMP5_d != &apos;-&apos;&lt;br/&gt;      AND a.SMP5_c   = &apos;-&apos;&lt;br/&gt;      THEN &apos;0,00&apos;&lt;br/&gt;      ELSE NVL(trim(TO_CHAR(ROUND(((a.SMP5_a / a.SMP5_b) * (a.SMP5_c / a.SMP5_d) * 100), 2), &apos;999990D99&apos;)), &apos;-&apos;)&lt;br/&gt;    END AS SMP5&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT agrupadores.Data,&lt;br/&gt;      agrupadores.CN,&lt;br/&gt;      &apos;PMM_TOTAL&apos; PMM,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_A, NULL, 0, dados.SMP5_A))) SMP5_a,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_B, NULL, 0, dados.SMP5_B))) SMP5_b,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_C, NULL, 0, dados.SMP5_C))) SMP5_c,&lt;br/&gt;      TO_CHAR(SUM(DECODE(dados.SMP5_D, NULL, 0, dados.SMP5_D))) SMP5_d&lt;br/&gt;    FROM dados_smp_CN dados,&lt;br/&gt;      agrupadores_smp_CN agrupadores&lt;br/&gt;    WHERE agrupadores.Data = dados.Data(+)&lt;br/&gt;    AND agrupadores.CN     = dados.CN(+)&lt;br/&gt;    AND agrupadores.Pmm    = dados.Pmm(+)&lt;br/&gt;    GROUP BY agrupadores.Data,&lt;br/&gt;      agrupadores.CN,&lt;br/&gt;      &apos;PMM_TOTAL&apos;&lt;br/&gt;    ) a&lt;br/&gt;  )&lt;br/&gt;SELECT TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA,&lt;br/&gt;  TO_CHAR(agrupadores_por_dados.CN) AS CN,&lt;br/&gt;  agrupadores_por_dados.Pmm,&lt;br/&gt;  agrupadores_por_dados.SMP5_a AS SMP_A,&lt;br/&gt;  agrupadores_por_dados.SMP5_b AS SMP_B,&lt;br/&gt;  agrupadores_por_dados.SMP5_c AS SMP_C,&lt;br/&gt;  agrupadores_por_dados.SMP5_d AS SMP_D,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN agrupadores_por_dados.SMP5          != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(agrupadores_por_dados.SMP5) &gt; 100&lt;br/&gt;    THEN &apos;100,00&apos;&lt;br/&gt;    ELSE REPLACE(agrupadores_por_dados.SMP5, &apos;.&apos;, &apos;,&apos;)&lt;br/&gt;  END AS SMP&lt;br/&gt;FROM agrupadores_por_dados&lt;br/&gt;ORDER BY 2,&lt;br/&gt;  1,&lt;br/&gt;  3</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;With dados_traducao_sem_vinculo As (Select VIEW_MSC_VINCULO_SMP5.DATA, VIEW_MSC_VINCULO_SMP5.CENTRAL, VIEW_MSC_VINCULO_SMP5.CN, VIEW_MSC_VINCULO_SMP5.PMM, VIEW_MSC_VINCULO_SMP5.REGIONAL, VIEW_MSC_VINCULO_SMP5.SMP5_A, VIEW_MSC_VINCULO_SMP5.SMP5_B, VIEW_MSC_VINCULO_SMP5.SMP5_C, VIEW_MSC_VINCULO_SMP5.SMP5_D, VIEW_MSC_VINCULO_SMP5.UF From VIEW_MSC_VINCULO_SMP5 Where VIEW_MSC_VINCULO_SMP5.CODIGO != &apos;NID&apos;), dados_smp_CN As (Select Data, Pmm, CN, Case When SMP5_B = &apos;0&apos; And SMP5_A != &apos;-&apos; Then &apos;0,00&apos; When SMP5_B = &apos;-&apos; And SMP5_A = &apos;-&apos; Then &apos;-&apos; When SMP5_B != &apos;-&apos; And SMP5_A = &apos;-&apos; Then &apos;0,00&apos; When SMP5_D = &apos;0&apos; And SMP5_C != &apos;-&apos; Then &apos;0,00&apos; When SMP5_D = &apos;-&apos; And SMP5_C = &apos;-&apos; Then &apos;-&apos; When SMP5_D != &apos;-&apos; And SMP5_C = &apos;-&apos; Then &apos;0,00&apos; Else NVL(trim(TO_CHAR(ROUND(((SMP5_A / SMP5_B) * (SMP5_C / SMP5_D) * 100), 2), &apos;999990D99&apos;)), &apos;-&apos;) End As SMP5, SMP5_A, SMP5_B, SMP5_C, SMP5_D From (Select TRUNC(Tab.DATA) As Data, pmm.NOME As Pmm, Tab.CN As CN, NVL(TO_CHAR(Sum(NVL(Tab.SMP5_A, 0))), &apos;-&apos;) As SMP5_A, NVL(TO_CHAR(Sum(NVL(Tab.SMP5_B, 0))), &apos;-&apos;) As SMP5_B, NVL(TO_CHAR(Sum(NVL(Tab.SMP5_C, 0))), &apos;-&apos;) As SMP5_C, NVL(TO_CHAR(Sum(NVL(Tab.SMP5_D, 0))), &apos;-&apos;) As SMP5_D From VIEW_MSC_CN_ATIVO_SMP5 b, dados_traducao_sem_vinculo Tab Inner Join PMM pmm On Tab.PMM = pmm.NOME Where Tab.CENTRAL = b.MSC And Tab.CN = b.CN And to_date(TO_CHAR(Tab.DATA, &apos;MM/yyyy&apos;), &apos;MM/yyyy&apos;) = b.PERIODO And b.STATUS = &apos;T&apos; Group By TRUNC(Tab.DATA), pmm.NOME, Tab.CN)), agrupadores_smp_CN As (Select * From (Select Distinct dados_smp_CN.Data From dados_smp_CN) datas, (Select Distinct dados_smp_CN.Pmm From dados_smp_CN) pmms, (Select Distinct dados_smp_CN.CN From dados_smp_CN) CNs), agrupadores_por_dados As (Select agrupadores.Data, agrupadores.CN, agrupadores.Pmm, DECODE(dados.SMP5_A, Null, &apos;-&apos;, dados.SMP5_A) SMP5_a, DECODE(dados.SMP5_B, Null, &apos;-&apos;, dados.SMP5_B) SMP5_b, DECODE(dados.SMP5_C, Null, &apos;-&apos;, dados.SMP5_C) SMP5_c, DECODE(dados.SMP5_D, Null, &apos;-&apos;, dados.SMP5_D) SMP5_d, DECODE(dados.SMP5, Null, &apos;-&apos;, dados.SMP5) SMP5 From dados_smp_CN dados, agrupadores_smp_CN agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.Pmm = dados.Pmm(+) And agrupadores.CN = dados.CN(+) Union All Select a.*, Case When a.SMP5_b = &apos;0&apos; And a.SMP5_a != &apos;-&apos; Then &apos;0,00&apos; When a.SMP5_b = &apos;-&apos; And a.SMP5_a = &apos;-&apos; Then &apos;-&apos; When a.SMP5_b != &apos;-&apos; And a.SMP5_a = &apos;-&apos; Then &apos;0,00&apos; When a.SMP5_d = &apos;0&apos; And a.SMP5_c != &apos;-&apos; Then &apos;0,00&apos; When a.SMP5_d = &apos;-&apos; And a.SMP5_c = &apos;-&apos; Then &apos;-&apos; When a.SMP5_d != &apos;-&apos; And a.SMP5_c = &apos;-&apos; Then &apos;0,00&apos; Else NVL(trim(TO_CHAR(ROUND(((a.SMP5_a / a.SMP5_b) * (a.SMP5_c / a.SMP5_d) * 100), 2), &apos;999990D99&apos;)), &apos;-&apos;) End As SMP5 From (Select agrupadores.Data, agrupadores.CN, &apos;PMM_TOTAL&apos; PMM, TO_CHAR(Sum(DECODE(dados.SMP5_A, Null, 0, dados.SMP5_A))) SMP5_a, TO_CHAR(Sum(DECODE(dados.SMP5_B, Null, 0, dados.SMP5_B))) SMP5_b, TO_CHAR(Sum(DECODE(dados.SMP5_C, Null, 0, dados.SMP5_C))) SMP5_c, TO_CHAR(Sum(DECODE(dados.SMP5_D, Null, 0, dados.SMP5_D))) SMP5_d From dados_smp_CN dados, agrupadores_smp_CN agrupadores Where agrupadores.Data = dados.Data(+) And agrupadores.CN = dados.CN(+) And agrupadores.Pmm = dados.Pmm(+) Group By agrupadores.Data, agrupadores.CN, &apos;PMM_TOTAL&apos;) a) Select TO_CHAR(agrupadores_por_dados.Data, &apos;dd/MM/yyyy&apos;) DATA, TO_CHAR(agrupadores_por_dados.CN) As CN, agrupadores_por_dados.Pmm, agrupadores_por_dado