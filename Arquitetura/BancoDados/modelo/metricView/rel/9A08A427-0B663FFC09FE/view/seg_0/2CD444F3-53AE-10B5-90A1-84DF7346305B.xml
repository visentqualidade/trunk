<userDefinedSQL>SELECT TO_CHAR(DATA, &apos;DD/MM/YYYY&apos;) AS DATA,&lt;br/&gt;  UF                               AS Uf,&lt;br/&gt;  NOME                             AS PMM,&lt;br/&gt;  SMP_A                            AS Smp_A,&lt;br/&gt;  SMP_B                            AS SMP_B,&lt;br/&gt;  (SELECT UF.NOME_EXTENSO FROM UF WHERE UF.NOME = UF&lt;br/&gt;  ) AS NOME,&lt;br/&gt;  CASE&lt;br/&gt;    WHEN SMP_B = &apos;-&apos;&lt;br/&gt;    AND SMP_A  = &apos;-&apos;&lt;br/&gt;    THEN &apos;-&apos;&lt;br/&gt;    WHEN SMP_B          != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(SMP_B) = 0&lt;br/&gt;    THEN &apos;0&apos;&lt;br/&gt;    WHEN (SMP_B         != &apos;-&apos;&lt;br/&gt;    AND TO_NUMBER(SMP_B) &gt; 0)&lt;br/&gt;    AND SMP_A            = &apos;-&apos;&lt;br/&gt;    THEN &apos;0&apos;&lt;br/&gt;    ELSE TO_CHAR(ROUND((SMP_A * 100 / SMP_B), 2))&lt;br/&gt;  END AS SMP&lt;br/&gt;FROM&lt;br/&gt;  (SELECT TRUNC(Tab.DATA) AS DATA,&lt;br/&gt;    Tab.UF,&lt;br/&gt;    pmm.NOME,&lt;br/&gt;    NVL(TO_CHAR(SUM(NVL(Tab.SAC_OK_MOC, 0) + NVL(Tab.SAC_OK_UCA, 0) + NVL(Tab.IVR_OK_MOC, 0) + NVL(Tab.IVR_OK_UCA1, 0) + NVL(Tab.IVR_OK_UCA2, 0) + NVL(Tab.CHAM_OK_ERIC, 0))), &apos;-&apos;)                                                                  AS SMP_A,&lt;br/&gt;    NVL(TO_CHAR(SUM(NVL(Tab.SAC_OK_MOC, 0) + NVL(Tab.SAC_OK_UCA, 0) + NVL(Tab.SAC_NOK, 0) + NVL(Tab.IVR_OK_MOC, 0) + NVL(Tab.IVR_OK_UCA1, 0) + NVL(Tab.IVR_OK_UCA2, 0) + NVL(Tab.IVR_NOK1, 0) + NVL(Tab.IVR_NOK2, 0) + NVL(Tab.CHAM_ERIC, 0))), &apos;-&apos;) AS SMP_B&lt;br/&gt;  FROM CDRVIEW_SMP3 Tab&lt;br/&gt;  INNER JOIN PMM pmm&lt;br/&gt;  ON To_Date(TO_CHAR(Tab.DATA, &apos;HH24:MI&apos;), &apos;HH24:MI&apos;) BETWEEN To_Date(TO_CHAR(pmm.INICIO, &apos;HH24:MI&apos;), &apos;HH24:MI&apos;) AND To_Date(TO_CHAR(pmm.FIM, &apos;HH24:MI&apos;), &apos;HH24:MI&apos;)&lt;br/&gt;  GROUP BY TRUNC(Tab.DATA),&lt;br/&gt;    Tab.UF,&lt;br/&gt;    pmm.NOME&lt;br/&gt;  )&lt;br/&gt;UNION&lt;br/&gt;SELECT TO_CHAR(DATA, &apos;DD/MM/YY